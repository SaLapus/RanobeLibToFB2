{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "http:allow-fetch",
    "http:allow-fetch-read-body",
    {
      "identifier": "http:default",
      "allow": [{ "url": "https://ranobelib.me" }]
    }
  ]
}
